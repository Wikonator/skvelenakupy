<script>
        jQuery(document).ready(function($){

        var slideTime = 350;

        window.onorientationchange = detectIPadOrientation;
        function detectIPadOrientation () {
            if ( orientation == 0 ) {
                if($(window).width() < 763) {
                    $('div .quick-access').children().find('.opener').removeClass('clicked');
                    $('.nav-container #nav').hide();
                    $('.main-nav .search-container').hide();
                    $('.nav-container #nav').hide();
                    $('.main-nav .search-container').hide();
                    $('.page-header #topCartContent').hide();
                }
            }
            else if ( orientation == 90 ) {
                if($(window).width() < 763) {
                    $('div .quick-access').children().find('.opener').removeClass('clicked');
                    $('.nav-container #nav').show();
                    $('.main-nav .search-container').show();
                    $('.page-header #topCartContent').hide();
                    $('.nav-container #nav').hide();
                    $('.main-nav .search-container').hide();
                    $('.page-header #topCartContent').hide();
                }
            }
            else if ( orientation == -90 ) {
                    if($(window).width() < 763) {
                        $('div .quick-access').children().find('.opener').removeClass('clicked');
                        $('.nav-container #nav').show();
                        $('.main-nav .search-container').show();
                        $('.page-header #topCartContent').hide();
                        $('.nav-container #nav').hide();
                        $('.main-nav .search-container').hide();
                        $('.page-header #topCartContent').hide();
                    }
            }
            else if ( orientation == 180 ) {
                if($(window).width() < 763) {
                    $('div .quick-access').children().find('.opener').removeClass('clicked');
                    $('.nav-container #nav').hide();
                    $('.main-nav .search-container').hide();
                    $('.nav-container #nav').hide();
                    $('.main-nav .search-container').hide();
                    $('.page-header #topCartContent').hide();
                }
            }
        }

        function activateOpener(el) {
            if ($(el).hasClass('clicked')) {
                    $(el).removeClass('clicked');
                    $(".page-header").css({"border" : "none"});
            }
            else {
                $('div .quick-access').children().find('.opener').removeClass('clicked');
                $(el).toggleClass('clicked');
                $(".page-header").css({"border-bottom" : "1px solid #000"});
            }
        }

        if ( $(window).width() <= 753) {
            if ( $("body").hasClass("customer-account") || $("body").hasClass("customer-account-create") || $("body").hasClass("customer-account-login") || $("body").hasClass("customer-account-logoutsuccess")){
                $('div .quick-access').children().find('.fc-user').addClass('customer-opened');
                $('div .quick-access .customer-opener').addClass('customer-opener-clicked');
                $(".page-header").css({"border" : "none"});
            }
            else{
                $('div .quick-access').children().find('.fc-user').removeClass('customer-opened');
                $('div .quick-access .customer-opener').removeClass('customer-opener-clicked');
            }
        }

        if ($(".breadcrumbs-wrapper").children().length == 0) {
            $(".breadcrumbs-wrapper").css({"border" : "none"});
        }



        $('#nav-opener').click(function(){
            activateOpener(this);
            $('#nav').slideToggle(slideTime);
            $('#topCartContent').slideUp(slideTime);
            $('.search-container').slideUp(slideTime);
        });
        $('#topcart-opener').click(function(){
            if($(window).width() < 753) {
                // mobile: full-width topcart flyout
                activateOpener(this);
                $('#nav').slideUp(slideTime);
                $('#topCartContent').appendTo('.page-header').slideToggle(slideTime);
                $('.search-container').slideUp(slideTime);
            }
        });
        $('#cartHeader').click(function(){
            if($(window).width() >= 753) {
                // desktop

                if( $('#topCartContent').is(':hidden') ) {
                    $(this).css({overflow: 'visible'});
                } else {
                    $('.reveal-modal-bg').fadeOut(slideTime);
                }

                $('#topCartContent').appendTo('#topCart .top-cart').slideToggle(slideTime,function(){
                    $(this).css({overflow: 'visible'});
                });
            }
        });
        $('#search-opener').click(function(){
            activateOpener(this);
            $('#nav').slideUp();
            $('#topCartContent').slideUp();
            $('.search-container').slideToggle();
        });

        $('#topcart-close').click(function(e){
            e.stopPropagation();
            $('#topcart-opener').removeClass('clicked');
            $('#topCartContent').slideUp(slideTime);
        });

        /*if(window.location.search.match(/adword=google/) || window.location.search.match(/start/)){
            jQuery.cookie('storeselector','de', { expires: 365, path: '/' })
        }*/

        if(!Mage.Cookies.get('storeselector') && window.location.hostname.match(/.de/) || window.location.search.match(/external/)) {
//            showStoreselector();
        }

        function showStoreselector() {
            $('#country-overlay').reveal({closeOnBackgroundClick: false});
        }
    });
</script>
